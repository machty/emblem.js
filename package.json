  "version":  "0.3.0",
  "homepage": "http://www.emblemjs.com/",
  "keywords": [
    "ember",
    "handlebars",
    "template",
    "html",
    "indentation"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/machty/emblem.js.git"
  },
  "engines": {
