  "version": "0.3.2",
  "homepage": "http://www.emblemjs.com/",
  "keywords": [
    "ember",
    "handlebars",
    "template",
    "html",
    "indentation"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/machty/emblem.js.git"
  },
  "engines": {
    "node": ">=0.4.7"
  },
  "dependencies": {
    "handlebars": ">= 1.0.10",
